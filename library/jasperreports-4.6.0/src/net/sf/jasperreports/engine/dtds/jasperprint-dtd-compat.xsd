 </xsd:element>

 <xsd:element name="textTruncateSuffix">
  <xsd:complexType mixed="true">
  </xsd:complexType>
 </xsd:element>

 <xsd:element name="lineBreakOffsets">
  <xsd:complexType mixed="true">
  </xsd:complexType>
 </xsd:element>

 <xsd:element name="frame">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="reportElement" minOccurs="0" maxOccurs="1"/>
    <xsd:element ref="box" minOccurs="0" maxOccurs="1"/>
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
     <xsd:element ref="line"/>
     <xsd:element ref="rectangle"/>
     <xsd:element ref="ellipse"/>
     <xsd:element ref="image"/>
     <xsd:element ref="text"/>
     <xsd:element ref="frame"/>
