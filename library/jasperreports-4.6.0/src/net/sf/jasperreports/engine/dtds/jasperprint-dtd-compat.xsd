  </xsd:complexType>
 </xsd:element>

 <xsd:element name="genericElementParameterValue">
  <xsd:complexType mixed="true">
   <xsd:attribute name="class" type="xsd:NMTOKEN" use="required"/>
  </xsd:complexType>
 </xsd:element>
</xsd:schema>
